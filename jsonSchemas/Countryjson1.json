      "columnName": "address2",
      "pattern": "^([A-Z][a-z]{3,10} [A-Z][a-z]{3,5}|[1-9]{2,5})$"
    },
    "address3": {
      "display": "Sector",
      "columnName": "address3",
      "pattern": "^[A-Z]-[1-9]/[1-9]$"
    },
    "region": {
      "display": "",
