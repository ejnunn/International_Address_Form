      "pattern": ""
    },
    "locale": {
      "display": "City - Code",
      "columnName": "locale",
      "pattern": "^[A-Z][a-z]{3,10}-[1-9]{6}$"
    },
    "postalCode": {
      "display": "",
      "columnName": "code",
      "pattern": ""
    },
    "required": [ "address1", "address2", "address3", "locale", "name" ]
  },
